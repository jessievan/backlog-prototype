%ul.objectives.unstyled
  - @plan.program.objectives.each do |objective|
    %li.objective
      .row
        .span3
          .handle.icon-align-justify
          = truncate(objective.title)
.actions
  = link_to "Re-Prioritize", @plan.program
  = link_to "New Objective", new_program_objective_path(@plan.program), class: 'pull-right'
